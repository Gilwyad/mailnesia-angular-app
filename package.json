{
  "name": "mailnesia-angular-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-with-dev-backend": "ng serve --proxy-config src/proxy.conf.json",
    "start-with-production-backend": "ng serve --proxy-config src/proxy-production.conf.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rimraf ./dist",
    "build:prod": "ng build --configuration production",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadlessCustom",
    "verify:ci": "npm run clean && npm run test:ci && npm run lint && npm run build:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.2.12",
